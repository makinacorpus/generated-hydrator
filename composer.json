{
    "name": "ocramius/generated-hydrator",
    "description": "An Object Hydrator that allows very fast array to object to array conversion",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Ocramius/GeneratedHydrator",
    "keywords": [
        "hydrator",
        "performance",
        "array conversion",
        "object conversion",
        "serializer"
    ],
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "require": {
        "php": "~7.0",
        "zendframework/zend-hydrator": "^2.0"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.13.0",
        "phpunit/phpunit": "^5.7.21",
        "squizlabs/php_codesniffer": "^3.0",
        "zendframework/zend-filter": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "GeneratedHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "GeneratedHydratorBenchmark\\":   "benchmarks","GeneratedHydratorTest\\":"tests",
            "GeneratedHydratorTestAsset\\":   "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
